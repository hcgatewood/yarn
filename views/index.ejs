  <head>
    <%- include partials/meta.ejs %>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel='Carousel-jQuery' href='/javascripts/Carousel_jQuery' />
    <link rel='stylesheet' href='/stylesheets/navbar.css' />
    <script type="text/javascript" src="javascripts/tmp-login.js"></script>

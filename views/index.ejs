  <body>
    <!--Overlay-->
    <div class="overlay"></div>
    <div id="modal">
      <div class="row">
            <h3 id="login_form">Sign in or create an account</h3>
      </div>
      <div class="col-lg-6">
          <div class="row" id="alignsignup_btn">
            <button class="signin_btn" id="b1" type="button">Sign in with Email</button>
        </div>

        <div class="row" id="alignsignup_btn">
            <button class="signin_btn" id="b2 "type="button">Sign in with Facebook</button>
        </div>

        <div class="row">
            <button class="signin_btn" type="button">Sign in with Google</button>
        </div>
      </div>

      <div class="col-lg-6">
        <form action='/' method='post' id='form'>
          <div class="row">
            <div class='form-group'>
                      <input  id="login_input" type='text' class='form-control' placeholder="First Name" name='firstname'>
                  </div>
              </div>
              <div class="row">
                  <div class='form-group'>
                      <input id="login_input" type='text' class='form-control' placeholder="Last Name" name='lastname'>
                  </div>
              </div>
              <div class="row">
                  <div class='form-group'>
                      <input id="login_input" type='text' class='form-control' placeholder="Username" name='username'>
                  </div>
              </div>
              <div class="row">
                  <div class='form-group'>
                      <input id="login_input" type='password' class='form-control'placeholder="Password" name='password'>
                  </div>
              </div>
              <div class="row">
                  <a href="http://localhost:3000/find">
                  <button id="login_button" type='submit' class="signin_btn">Login</button>
                  </a>
                </div>
            </form>
      </div>
    </div>
    <%- include partials/navbar.ejs %>

    <!--Brief Info-->
    <div class="Info">
      <p>Start creating stories
        with your friends,<br> make new ones,
        <br>and share your creations with the world!<br>
      </p>
  </div>


